        let localData={
            ageGroup:"",
            sort:"",
            category:"",
        }
        if(localStorage.getItem("localData")){
            let tempData=JSON.parse(localStorage.getItem("localData"));
            tempData[key]=value;
            localStorage.setItem("localData",JSON.stringify(tempData))
        }else{
            localData[key]=value;
            localStorage.setItem("localData",JSON.stringify(localData))
        }
    }
      function remove(val) {
