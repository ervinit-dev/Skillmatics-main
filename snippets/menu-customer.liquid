<style>
  .more-content ul li a {
    font-family: 'Avenir LT Std 65 Medium' !important;
  }
</style>
{%- if item.show_customer_links -%}
<!-- tt-account -->
<div class="tt-desctop-parent-account tt-parent-box">
  <div class="tt-account tt-dropdown-obj">
    <button class="tt-dropdown-toggle"
            data-tooltip="{{ 'general.tooltip_texts.header_account' | t }}"
            data-tposition="bottom"
            >
      {% if settings.header_design == "header_type_5" %}
      {% if item.show_account_icon %}<i class="icon-f-94"></i>{% endif %}
      {% if item.account_icon_text != "" %}<span class="tt-dropdown-toggle__text">{{ item.account_icon_text }}</span>{% endif %}
      {% else %}<i class="icon-f-94"></i>
      {% endif %}
    </button>
    <div class="tt-dropdown-menu">
      <div class="tt-mobile-add">
        <button class="tt-close">{{ "layout.customer.close" | t }}</button>
      </div>
      <div class="tt-dropdown-inner more-content">
        <ul>
          <li><a href="/pages/about-us">About Us</a></li>
          <li><a href="/pages/faqs">FAQs</a></li>
          <li><a href="/pages/contact">Contact us</li>
          
          {% if settings.show_wishlist %}<li><a href="/pages/wishlist">{% include "svg-heart" %}{{ "wishlist.header.wishlist" | t }}</a></li>{% endif %}
          {% if settings.show_compare %}<li><a href="/pages/compare">{% include "svg-compare" %}{{ "compare.header.compare" | t }}</a></li>{% endif %}
        </ul>
      </div>
    </div>
  </div>
</div>
<!-- /tt-account -->
{%- endif -%}